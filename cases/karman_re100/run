gcc   -shared -o libvkvs.so -fPIC vkvs.c

exe=../../build/rbvms/rbvms
mesh=von-karman-nurbs2.mesh 
##mesh=von-karman-tet.msh

rm output* log*

mpirun -n 16 $exe -m $mesh -o 2 -r 5 \
--outflow-bdr 4  --weak-bdr 3 -l libvkvs.so \
-s 45 -dt 0.05 --dt_vis 0.5 -tf 1000 \
-lt 1e-3 -ni 10  -ri 10  | tee log0
